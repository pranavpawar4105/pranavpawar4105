#include <iostream>
const double pi = 2.14159265358979323846;
using namespace std;
//defined functions

float findArea(float r)
{
    return (pi * r * r);
}
// calling function by "int" below 
int main()
{
    float r, Area;
    r = 6;

    Area = findArea(r);
    
    cout << "Area of Circle is :" << Area;
    return 0;
}
